{
  "name": "electron-vue3-vite",
  "version": "0.0.0",
  "scripts": {
    "dev": "concurrently -n=vite,main -c=green,blue \"npm run dev:vite\" \"npm run dev:main\"",
    "build": "npm run build:vite && npm run build:main",
    "builder": "npm run build:vite && cross-env-shell IS_BUILDER=builder npm run build:main && electron-builder",
    "dev:main": "cross-env NODE_ENV=development node build/dev.js",
    "build:main": "cross-env NODE_ENV=production node build/build.js",
    "dev:vite": "cross-env NODE_ENV=development vite",
    "build:vite": "cross-env NODE_ENV=production vite build"
  },
  "main": "dist/main.js",
  "dependencies": {
    "axios": "^0.20.0",
    "core-js": "^3.6.5",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0-0",
    "vuex": "^4.0.0-0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@vue/compiler-sfc": "^3.0.0",
    "babel-loader": "^8.1.0",
    "chalk": "^4.1.0",
    "concurrently": "^5.3.0",
    "copy-webpack-plugin": "^6.1.1",
    "cross-env": "^7.0.2",
    "electron": "^10.1.2",
    "electron-builder": "^22.8.1",
    "electron-devtools-installer": "^3.1.1",
    "happypack": "^5.0.1",
    "node-loader": "^1.0.1",
    "ora": "^5.1.0",
    "portfinder": "^1.0.28",
    "rimraf": "^3.0.2",
    "vite": "^1.0.0-rc.4",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12"
  }
}
